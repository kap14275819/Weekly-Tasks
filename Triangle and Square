#include <iostream>

using namespace std;

int main() {
	int x1, y1, x2, y2, x3, y3, x4, y4;
	int count = 1;
	
	char end;
	cout << "1st Coord = ";
	cin >> x1;
	cin >> y1;
	cout << "\nNext? ";
	cin >> end;
	
	if(end == 'y') {
		cout << "\n2nd Coord = ";
		cin >> x2;
		cin >> y2;
		count++;
	}
	
	cout << "\nNext? ";
	cin >> end;
	
	if(end == 'y') {
		cout << "\n3rd Coord = ";
		cin >> x3;
		cin >> y3;
		count++;
	}
	
	cout << "\nNext? ";
	cin >> end;
	
	if(end == 'y') {
		cout << "\n4th Coord = ";
		cin >> x4;
		cin >> y4;
		count++;
	}
	
	if(count==3 && (x1 != x2 && x1 != x3 && x2 != x3) || (y1 != y2 && y1 != y3 && y2 != y3)) {
		cout << "TRIANGLE\n";
	}
	
	if(count == 4 && (x1 != x2 && x1 != x3 && x1 != x4 && x2 != x3 && x3 !=x4) || (y1 != y2 && y1 != y3 && y1 != y4 && y2 != x3 && y3 !=y4)) {
		cout << "SQUARE\n";
	}
	
	return 0;
}
